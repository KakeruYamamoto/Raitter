<h1>ブログ一覧</h1>

<table>
  <tr>
    <th>CONTENT</th>
  </tr>

<% @raeets.each do |raeet| %>
  <tr>
    <td><%= raeet.content %></td>
    <td><%= link_to '詳細', raeet_path(raeet.id) %></td>
    <td><%= link_to '編集', edit_raeet_path(raeet.id) %></td>
  </tr>
<% end %>
</table>
<br>
<%= link_to '新しくブログを投稿する',new_raeet_path %>
